{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "tx_id",
    "type",
    "version",
    "origin",
    "survey_id",
    "flushed",
    "submitted_at",
    "collection",
    "metadata",
    "response_id",
    "questionnaire_id",
    "case_type",
    "region_code",
    "started_at",
    "case_id",
    "data"
  ],
  "properties": {
    "tx_id": {
      "$id": "#/properties/tx_id",
      "type": "string",
      "title": "The Tx_id Schema",
      "default": "",
      "examples": [
        "411e12a2-e57f-4f07-917d-76c93b422460"
      ],
      "pattern": "^(.*)$"
    },
    "type": {
      "$id": "#/properties/type",
      "type": "string",
      "title": "The Type Schema",
      "default": "",
      "examples": [
        "xxxxx"
      ],
      "pattern": "^(.*)$"
    },
    "version": {
      "$id": "#/properties/version",
      "type": "string",
      "title": "The Version Schema",
      "default": "",
      "examples": [
        "0.0.3"
      ],
      "pattern": "^(.*)$"
    },
    "origin": {
      "$id": "#/properties/origin",
      "type": "string",
      "title": "The Origin Schema",
      "default": "",
      "examples": [
        "yyyyy"
      ],
      "pattern": "^(.*)$"
    },
    "survey_id": {
      "$id": "#/properties/survey_id",
      "type": "string",
      "title": "The Survey_id Schema",
      "default": "",
      "examples": [
        "0"
      ],
      "pattern": "^(.*)$"
    },
    "flushed": {
      "$id": "#/properties/flushed",
      "type": "boolean",
      "title": "The Flushed Schema",
      "default": false,
      "examples": [
        false
      ]
    },
    "submitted_at": {
      "$id": "#/properties/submitted_at",
      "type": "string",
      "title": "The Submitted_at Schema",
      "default": "",
      "examples": [
        "2019-08-21T12:51:57.119678"
      ],
      "pattern": "^(.*)$"
    },
    "collection": {
      "$id": "#/properties/collection",
      "type": "object",
      "title": "The Collection Schema",
      "required": [
        "exercise_sid",
        "schema_name",
        "period"
      ],
      "properties": {
        "exercise_sid": {
          "$id": "#/properties/collection/properties/exercise_sid",
          "type": "string",
          "title": "The Exercise_sid Schema",
          "default": "",
          "examples": [
            "1be356de-bde7-48bf-b169-00e0b3822a9c"
          ],
          "pattern": "^(.*)$"
        },
        "schema_name": {
          "$id": "#/properties/collection/properties/schema_name",
          "type": "string",
          "title": "The Schema_name Schema",
          "default": "",
          "examples": [
            "test_relationships"
          ],
          "pattern": "^(.*)$"
        },
        "period": {
          "$id": "#/properties/collection/properties/period",
          "type": "string",
          "title": "The Period Schema",
          "default": "",
          "examples": [
            "201605"
          ],
          "pattern": "^(.*)$"
        }
      }
    },
    "metadata": {
      "$id": "#/properties/metadata",
      "type": "object",
      "title": "The Metadata Schema",
      "required": [
        "user_id",
        "ru_ref"
      ],
      "properties": {
        "user_id": {
          "$id": "#/properties/metadata/properties/user_id",
          "type": "string",
          "title": "The User_id Schema",
          "default": "",
          "examples": [
            "UNKNOWN"
          ],
          "pattern": "^(.*)$"
        },
        "ru_ref": {
          "$id": "#/properties/metadata/properties/ru_ref",
          "type": "string",
          "title": "The Ru_ref Schema",
          "default": "",
          "examples": [
            "34942807969m"
          ],
          "pattern": "^(.*)$"
        }
      }
    },
    "response_id": {
      "$id": "#/properties/response_id",
      "type": "string",
      "title": "The Response_id Schema",
      "default": "",
      "examples": [
        "3387746373631872"
      ],
      "pattern": "^(.*)$"
    },
    "questionnaire_id": {
      "$id": "#/properties/questionnaire_id",
      "type": "string",
      "title": "The Questionnaire_id Schema",
      "default": "",
      "examples": [
        "7539657418968524"
      ],
      "pattern": "^(.*)$"
    },
    "case_type": {
      "$id": "#/properties/case_type",
      "type": "string",
      "title": "The Case_type Schema",
      "default": "",
      "examples": [
        "HI"
      ],
      "pattern": "^(.*)$"
    },
    "region_code": {
      "$id": "#/properties/region_code",
      "type": "string",
      "title": "The Region_code Schema",
      "default": "",
      "examples": [
        "GB-ENG"
      ],
      "pattern": "^(.*)$"
    },
    "started_at": {
      "$id": "#/properties/started_at",
      "type": "string",
      "title": "The Started_at Schema",
      "default": "",
      "examples": [
        "2019-08-21T12:51:18.332714"
      ],
      "pattern": "^(.*)$"
    },
    "case_id": {
      "$id": "#/properties/case_id",
      "type": "string",
      "title": "The Case_id Schema",
      "default": "",
      "examples": [
        "5515cd5f-fcff-44f0-ac02-3530477693ea"
      ],
      "pattern": "^(.*)$"
    },
    "data": {
      "$id": "#/properties/data",
      "type":  "object",
      "title": "The Data Schema",
      "items": {
        "$id": "#/properties/data/items",
        "type": "object",
        "title": "The Items Schema",
        "required": [
          "answer_id",
          "value"
        ],
        "properties": {
          "answer_id": {
            "$id": "#/properties/data/items/properties/answer_id",
            "type": "string",
            "title": "The Answer_id Schema",
            "default": "",
            "examples": [
              "first-name"
            ],
            "pattern": "^(.*)$"
          },
          "value": {
            "$id": "#/properties/data/items/properties/value",
            "type": ["array","string"],
            "title": "The Value Schema",
            "default": "",
            "examples": [
              "Joe"
            ],
            "pattern": "^(.*)$"
          },
          "list_item_id": {
            "$id": "#/properties/data/items/properties/list_item_id",
            "type": "string",
            "title": "The List_item_id Schema",
            "default": "",
            "examples": [
              "Whujql"
            ],
            "pattern": "^(.*)$"
          }
        }
      }
    }
  }
}
