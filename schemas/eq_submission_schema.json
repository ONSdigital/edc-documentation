{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "tx_id",
    "type",
    "version",
    "origin",
    "survey_id",
    "flushed",
    "submitted_at",
    "collection",
    "metadata",
    "response_id",
    "questionnaire_id",
    "started_at",
    "case_id",
    "data"
  ],
  "properties": {
    "tx_id": {
      "$id": "#/properties/tx_id",
      "type": "string",
      "title": "The Tx_id Schema",
      "default": "",
      "examples": [
        "aaaaaaaa-bbbb-cccc-dddd-eeeeeeeeeeee"
      ],
      "pattern": "^(.*)$"
    },
    "type": {
      "$id": "#/properties/type",
      "type": "string",
      "title": "The Type Schema",
      "default": "",
      "examples": [
        "xxxx"
      ],
      "pattern": "^(.*)$"
    },
    "version": {
      "$id": "#/properties/version",
      "type": "string",
      "title": "The Version Schema",
      "default": "",
      "examples": [
        "0.0.3"
      ],
      "pattern": "^(.*)$"
    },
    "origin": {
      "$id": "#/properties/origin",
      "type": "string",
      "title": "The Origin Schema",
      "default": "",
      "examples": [
        "yyyy"
      ],
      "pattern": "^(.*)$"
    },
    "survey_id": {
      "$id": "#/properties/survey_id",
      "type": "string",
      "title": "The Survey_id Schema",
      "default": "",
      "examples": [
        "0"
      ],
      "pattern": "^(.*)$"
    },
    "flushed": {
      "$id": "#/properties/flushed",
      "type": "boolean",
      "title": "The Flushed Schema",
      "default": false,
      "examples": [
        false
      ]
    },
    "submitted_at": {
      "$id": "#/properties/submitted_at",
      "type": "string",
      "title": "The Submitted_at Schema",
      "default": "",
      "examples": [
        "2019-08-19T11:25:43.145876"
      ],
      "pattern": "^(.*)$"
    },
    "collection": {
      "$id": "#/properties/collection",
      "type": "object",
      "title": "The Collection Schema",
      "required": [
        "exercise_sid",
        "schema_name",
        "period"
      ],
      "properties": {
        "exercise_sid": {
          "$id": "#/properties/collection/properties/exercise_sid",
          "type": "string",
          "title": "The Exercise_sid Schema",
          "default": "",
          "examples": [
            "8505f4a5-21cc-4b52-aa79-8930e9e9a888"
          ],
          "pattern": "^(.*)$"
        },
        "schema_name": {
          "$id": "#/properties/collection/properties/schema_name",
          "type": "string",
          "title": "The Schema_name Schema",
          "default": "",
          "examples": [
            "test_relationships_primary"
          ],
          "pattern": "^(.*)$"
        },
        "period": {
          "$id": "#/properties/collection/properties/period",
          "type": "string",
          "title": "The Period Schema",
          "default": "",
          "examples": [
            "201605"
          ],
          "pattern": "^(.*)$"
        }
      }
    },
    "metadata": {
      "$id": "#/properties/metadata",
      "type": "object",
      "title": "The Metadata Schema",
      "required": [
        "user_id",
        "ru_ref"
      ],
      "properties": {
        "user_id": {
          "$id": "#/properties/metadata/properties/user_id",
          "type": "string",
          "title": "The User_id Schema",
          "default": "",
          "examples": [
            "UNKNOWN"
          ],
          "pattern": "^(.*)$"
        },
        "ru_ref": {
          "$id": "#/properties/metadata/properties/ru_ref",
          "type": "string",
          "title": "The Ru_ref Schema",
          "default": "",
          "examples": [
            "68881731801T"
          ],
          "pattern": "^(.*)$"
        }
      }
    },
    "response_id": {
      "$id": "#/properties/response_id",
      "type": "string",
      "title": "The Response_id Schema",
      "default": "",
      "examples": [
        "7822783472717202"
      ],
      "pattern": "^(.*)$"
    },
    "questionnaire_id": {
      "$id": "#/properties/questionnaire_id",
      "type": "string",
      "title": "The Questionnaire_id Schema",
      "default": "",
      "examples": [
        "3131347216561302"
      ],
      "pattern": "^(.*)$"
    },
    "started_at": {
      "$id": "#/properties/started_at",
      "type": "string",
      "title": "The Started_at Schema",
      "default": "",
      "examples": [
        "2019-08-19T11:25:18.676780"
      ],
      "pattern": "^(.*)$"
    },
    "case_id": {
      "$id": "#/properties/case_id",
      "type": "string",
      "title": "The Case_id Schema",
      "default": "",
      "examples": [
        "91b9ae45-2f56-4f9c-a923-56718c6a4b7d"
      ],
      "pattern": "^(.*)$"
    },
    "data": {
      "$id": "#/properties/data",
      "type": "object",
      "title": "The Data Schema",
      "required": [
        "answers",
        "lists"
      ],
      "properties": {
        "answers": {
          "$id": "#/properties/data/properties/answers",
          "type": "array",
          "title": "The Answers Schema",
          "items": {
            "$id": "#/properties/data/properties/answers/items",
            "type": "object",
            "title": "The Items Schema",
            "required": [
              "answer_id",
              "value"
            ],
            "properties": {
              "answer_id": {
                "$id": "#/properties/data/properties/answers/items/properties/answer_id",
                "type": "string",
                "title": "The Answer_id Schema",
                "default": "",
                "examples": [
                  "first-name"
                ],
                "pattern": "^(.*)$"
              },
              "value": {
                "$id": "#/properties/data/properties/answers/items/properties/value",
                "type": ["string", "array"],
                "title": "The Value Schema",
                "default": "",
                "examples": [
                  "John"
                ],
                "pattern": "^(.*)$"
              },
              "list_item_id": {
                "$id": "#/properties/data/properties/answers/items/properties/list_item_id",
                "type": "string",
                "title": "The List_item_id Schema",
                "default": "",
                "examples": [
                  "yIluIy"
                ],
                "pattern": "^(.*)$"
              }
            }
          }
        },
        "lists": {
          "$id": "#/properties/data/properties/lists",
          "type": "array",
          "title": "The Lists Schema",
          "items": {
            "$id": "#/properties/data/properties/lists/items",
            "type": "object",
            "title": "The Items Schema",
            "required": [
              "items",
              "name"
            ],
            "properties": {
              "items": {
                "$id": "#/properties/data/properties/lists/items/properties/items",
                "type": "array",
                "title": "The Items Schema",
                "items": {
                  "$id": "#/properties/data/properties/lists/items/properties/items/items",
                  "type": "string",
                  "title": "The Items Schema",
                  "default": "",
                  "examples": [
                    "zGBdpb",
                    "cWGwcF"
                  ],
                  "pattern": "^(.*)$"
                }
              },
              "name": {
                "$id": "#/properties/data/properties/lists/items/properties/name",
                "type": "string",
                "title": "The Name Schema",
                "default": "",
                "examples": [
                  "people"
                ],
                "pattern": "^(.*)$"
              }
            }
          }
        }
      }
    }
  }
}
